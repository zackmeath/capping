-- Name: studentmajors_stid_fkey; Type: FK CONSTRAINT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY studentmajors
    ADD CONSTRAINT studentmajors_stid_fkey FOREIGN KEY (stid) REFERENCES student(stid);


