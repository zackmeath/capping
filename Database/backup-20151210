--
-- Name: course_pkey; Type: CONSTRAINT; Schema: public; Owner: postgres; Tablespace: 
--

ALTER TABLE ONLY course
    ADD CONSTRAINT course_pkey PRIMARY KEY (cid);


--
-- Name: coursestaken_pkey; Type: CONSTRAINT; Schema: public; Owner: postgres; Tablespace: 
--

ALTER TABLE ONLY coursestaken
    ADD CONSTRAINT coursestaken_pkey PRIMARY KEY (stid, cid);


--
-- Name: equivalent_pkey; Type: CONSTRAINT; Schema: public; Owner: postgres; Tablespace: 
--

ALTER TABLE ONLY equivalent
    ADD CONSTRAINT equivalent_pkey PRIMARY KEY (cid, rid);


--
-- Name: major_pkey; Type: CONSTRAINT; Schema: public; Owner: postgres; Tablespace: 
--

ALTER TABLE ONLY major
    ADD CONSTRAINT major_pkey PRIMARY KEY (mid);


--
-- Name: major_title_key; Type: CONSTRAINT; Schema: public; Owner: postgres; Tablespace: 
--

ALTER TABLE ONLY major
    ADD CONSTRAINT major_title_key UNIQUE (title);


--
-- Name: majorrequirement_pkey; Type: CONSTRAINT; Schema: public; Owner: postgres; Tablespace: 
--

ALTER TABLE ONLY majorrequirement
    ADD CONSTRAINT majorrequirement_pkey PRIMARY KEY (mid, rid);


--
-- Name: requirement_pkey; Type: CONSTRAINT; Schema: public; Owner: postgres; Tablespace: 
--

ALTER TABLE ONLY requirement
    ADD CONSTRAINT requirement_pkey PRIMARY KEY (rid);


--
-- Name: school_pkey; Type: CONSTRAINT; Schema: public; Owner: postgres; Tablespace: 
--

ALTER TABLE ONLY school
    ADD CONSTRAINT school_pkey PRIMARY KEY (scid);


--
-- Name: student_pkey; Type: CONSTRAINT; Schema: public; Owner: postgres; Tablespace: 
--

ALTER TABLE ONLY student
    ADD CONSTRAINT student_pkey PRIMARY KEY (stid);


--
-- Name: studentmajors_pkey; Type: CONSTRAINT; Schema: public; Owner: postgres; Tablespace: 
--

ALTER TABLE ONLY studentmajors
    ADD CONSTRAINT studentmajors_pkey PRIMARY KEY (stid, mid);


--
-- Name: users_email_key; Type: CONSTRAINT; Schema: public; Owner: postgres; Tablespace: 
--

ALTER TABLE ONLY users
    ADD CONSTRAINT users_email_key UNIQUE (email);


--
-- Name: users_pkey; Type: CONSTRAINT; Schema: public; Owner: postgres; Tablespace: 
--

ALTER TABLE ONLY users
    ADD CONSTRAINT users_pkey PRIMARY KEY (uid);


--
-- Name: course_scid_fkey; Type: FK CONSTRAINT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY course
    ADD CONSTRAINT course_scid_fkey FOREIGN KEY (scid) REFERENCES school(scid);


--
-- Name: coursestaken_cid_fkey; Type: FK CONSTRAINT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY coursestaken
    ADD CONSTRAINT coursestaken_cid_fkey FOREIGN KEY (cid) REFERENCES course(cid);


--
-- Name: coursestaken_stid_fkey; Type: FK CONSTRAINT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY coursestaken
    ADD CONSTRAINT coursestaken_stid_fkey FOREIGN KEY (stid) REFERENCES student(stid);


--
-- Name: equivalent_cid_fkey; Type: FK CONSTRAINT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY equivalent
    ADD CONSTRAINT equivalent_cid_fkey FOREIGN KEY (cid) REFERENCES course(cid);


--
-- Name: equivalent_rid_fkey; Type: FK CONSTRAINT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY equivalent
    ADD CONSTRAINT equivalent_rid_fkey FOREIGN KEY (rid) REFERENCES requirement(rid);


--
-- Name: majorrequirement_mid_fkey; Type: FK CONSTRAINT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY majorrequirement
    ADD CONSTRAINT majorrequirement_mid_fkey FOREIGN KEY (mid) REFERENCES major(mid);


--
-- Name: majorrequirement_rid_fkey; Type: FK CONSTRAINT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY majorrequirement
    ADD CONSTRAINT majorrequirement_rid_fkey FOREIGN KEY (rid) REFERENCES requirement(rid);


--
-- Name: student_currentcollege_fkey; Type: FK CONSTRAINT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY student
    ADD CONSTRAINT student_currentcollege_fkey FOREIGN KEY (currentcollege) REFERENCES school(scid);


