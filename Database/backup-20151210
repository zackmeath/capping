-- Name: student_stid_fkey; Type: FK CONSTRAINT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY student
    ADD CONSTRAINT student_stid_fkey FOREIGN KEY (stid) REFERENCES users(uid);


